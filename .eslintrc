{
  "env": {
      "browser": false,
      "commonjs": true,
      "es6": true
  },
  "extends": [
      "airbnb"
    ],
  "parserOptions": {
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true,
          "jsx": true
      },
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "parser": "babel-eslint"
}